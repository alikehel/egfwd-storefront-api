        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "jasmine": "^4.4.0",
        "jasmine-spec-reporter": "^7.0.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "pg": "^8.8.0",
        "supertest": "^6.2.4"
